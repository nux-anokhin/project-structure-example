{
    "name": "nuxgame/project-structure-example",
    "require": {
        "php": "^8.3",
        "laravel/framework": "12.*",
        "laravel/sanctum": "4.*",
        "laravel/socialite": "5.*",
        "laravel/tinker": "2.*",
        "laravel/ui": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "12.*"
    },
    "autoload": {
        "psr-4": {
            "Example\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
